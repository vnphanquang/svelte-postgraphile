{
  "name": "template",
  "description": "Template for Typescript Svelte PostgreSQL-GraphQL (Postgraphile) with TailwindCSS",
  "keywords": [
    "sapper",
    "svelte",
    "postgraphile",
    "postgreSQL",
    "graphQL",
    "tailwind",
    "postcss",
    "typescript"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vnphanquang/svelte-postgraphile",
  "repository": {
    "type": "git",
    "url": "https://github.com/vnphanquang/svelte-postgraphile"
  },
  "version": "0.0.1",
  "scripts": {
    "postinstall": "bash ./scripts/setup.bash",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./**/*.js ./src/**/*.ts",
    "dev": "run-p dev:*",
    "dev:sapper": "sapper dev --port 5252",
    "dev:graphql": "yarn codegen:graphql --watch src/services/graphql/generated/schema.graphql",
    "dev:validate": "yarn validate --watch",
    "dev:css": "yarn build:css --watch --verbose",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "codegen:graphql": "graphql-codegen --config codegen.graphql.yaml",
    "bundle:production": "cross-env NODE_ENV=production yarn build",
    "bundle:dev": "cross-env NODE_ENV=development yarn build",
    "prebuild": "yarn install && yarn codegen:graphql && yarn validate",
    "build": "run-s build:*",
    "build:css": "postcss src/theme/styles --dir static/styles --ext css",
    "build:sapper": "sapper build --port 5252 --legacy",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0",
    "@graphile/operation-hooks": "^1.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "postgraphile": "^4.9.0",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@beyonk/sapper-rbac": "^0.0.3",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/jsdoc": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tailwindcss/typography": "^0.3.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "add": "^2.0.6",
    "autoprefixer": "^10.0.3",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.9.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "graphile-build": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.0.0",
    "postcss-import": "^13.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.27.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-toml": "^1.0.0",
    "sapper": "^0.28.8",
    "svelte": "^3.29.0",
    "svelte-awesome": "^2.3.0",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "4.5.2",
    "tailwindcss": "^2.0.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "yarn": "^1.22.10"
  }
}
